<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include user data -->
        <include domain="file" path="." />
        <!-- Exclude cache directories -->
        <exclude domain="file" path="cache" />
        <exclude domain="file" path="code_cache" />
    </cloud-backup>
</data-extraction-rules>